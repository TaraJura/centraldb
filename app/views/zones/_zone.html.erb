<div id="<%= dom_id zone %>">
 <p>
    <strong>Zone ID:</strong>
    <%= zone.id %>
  </p>
  <p>
    <strong>Name:</strong>
    <%= zone.name %>
  </p>
  <p>
    <strong>Visitors with acces by ID:</strong>
    <%= zone.visitorsZoneAcce.map{|visitorsZoneAcce| visitorsZoneAcce.visitor_id}.join(", ") %>
  </p>
</div>
